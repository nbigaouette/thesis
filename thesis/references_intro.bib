	mendeley-tags = {FEL,atomic\_density,cluster\_ions,collisions,electron\_dynamics,electron\_emission,electrons,emission\_line,energy\_absorption,equilibration,extended\_systems,inner\_shell,laser\_plasma,low\_frequency,photon\_excitation,pulses,xrays},
	month = aug,
	number = {8},
	title = {{Fast electrons from multi-electron dynamics in xenon clusters induced by inner-shell ionization}},
