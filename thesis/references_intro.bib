	pages = {446--449},
	title = {{A hierarchical O(N log N) force-calculation algorithm}},
	type = {Journal article},
	url = {http://www.nature.com/doifinder/10.1038/324446a0},
	volume = {324},
	year = {1986}
}

@incollection{Gibbon2002,
